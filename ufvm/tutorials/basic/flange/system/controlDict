FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     uFVM;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         3;

deltaT          0.1;

writeControl    timeStep;

writeInterval   5;

purgeWrite      1;


