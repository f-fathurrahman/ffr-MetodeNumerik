FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     uFVM;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         1000;

deltaT          10;

writeControl    timeStep;

writeInterval   10;

purgeWrite      1;


